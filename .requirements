# these are set by kong core, some used in setup_env.sh and kong-build-tools
# kong-distributions also uses them, and translates them into the naming
# convention over there

KONG_PACKAGE_NAME=kong
KONG_CONFLICTS=kong-enterprise-edition
KONG_LICENSE="ASL 2.0"

KONG_GMP_VERSION=6.1.2
RESTY_VERSION=1.15.8.2
RESTY_LUAROCKS_VERSION=3.2.1
RESTY_OPENSSL_VERSION=1.1.1d
RESTY_PCRE_VERSION=8.43
LIBYAML_VERSION=0.2.2
KONG_GO_PLUGINSERVER_VERSION=v0.3.0

# here go enterprise build specific ones which might or might not have the
# same names as core for building the final package.

KONG_NGINX_MODULE_BRANCH=master
KONG_BUILD_TOOLS_BRANCH=master

KONG_DEP_NETTLE_VERSION=3.4.1
KONG_DEP_LIBYAML_VERSION=0.2.2
KONG_DEP_LICENSING_VERSION=0.1.3
KONG_DEP_PASSWDQC_VERSION=1.3.1

# opentracing stuff
KONG_DEP_OPENTRACING_CPP_VERSION=v1.5.1
KONG_DEP_DATADOG_VERSION=v1.1.2
KONG_DEP_NGINX_OPENTRACING_VERSION=v0.9.0
KONG_DEP_JAEGER_VERSION=v0.5.0

LYAML_VERSION=6.2.4
