KONG_NGINX_MODULE_BRANCH=0.0.8
KONG_BUILD_TOOLS_BRANCH=4.12.1

KONG_DISTRIBUTIONS_VERSION=master
KONG_DOCKER_KONG_VERSION=master

KONG_DEP_NETTLE_VERSION=3.5.1
KONG_DEP_LIBYAML_VERSION=0.2.5
KONG_DEP_LICENSING_VERSION=0.1.4
KONG_DEP_PASSWDQC_VERSION=1.3.1

# opentracing stuff
